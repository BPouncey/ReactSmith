[state, dispatch] = useReducer(reducer, initialState)\n  appContext.dispatch = dispatch\n\n  return (\n    <AppContext.Provider style={defaultContext} >\n      <Router>\n        {/*Header here*/}\n        <Switch>\n\n          <Route path=\"/\" exact>\n            <Home theme={setTheme(state.theme)} />\n          </Route>\n\n          <Route path=\"/home\">\n            <Redirect to=\"/\" />\n          </Route>\n\n          <Route path='/'>\n            <div>Page not found</div>\n          </Route>\n\n        </Switch>\n        {/*Footer here*/}\n      </Router>\n    </AppContext.Provider>\n  )\n}\n\nexport default App"},"lineCount":null}},"error":null,"hash":"196438a65cc2e7eedb272ff2e3145f1f","cacheData":{"env":{}}}