atus === \"sending\" && <div style={{ color: \"blue\" }}>sending...</div>}\n        {status === \"error\" && (\n          <div style={{ color: \"red\" }}>\n            \"error\"\n          </div>\n        )}\n        {status === \"success\" && (\n          <div style={{ color: \"green\" }}>\n            \"success\"\n          </div>\n        )}\n        <input\n          ref={node => (name = node)}\n          type=\"text\"\n          placeholder=\"Your name\"\n        />\n        <br />\n        <input\n          ref={node => (email = node)}\n          type=\"email\"\n          placeholder=\"Your email\"\n        />\n        <br />\n        <button onClick={submit}>\n          Submit\n        </button>\n      </div>\n    );\n  };\n\n  return (\n    <div>\n      <MailchimpSubscribe\n        url={url}\n        render={({ subscribe, status, message }) => (\n          <CustomForm\n            status={status}\n            message={message}\n            onValidated={formData => subscribe(formData)}\n          />\n        )}\n      />\n    </div>\n  )\n}\n\nexport default SignupForm\n"},"lineCount":null}},"error":null,"hash":"48d68a507c483a51973bc56040f725e5","cacheData":{"env":{"MAILCHIMP":"https://yahoo.us4.list-manage.com/subscribe/post?u=55bf933193762c4b4283dd7f8&amp;id=3e3a1660ab"}}}